	year = {2015},
	keywords = {abstract ok, Artificial neural networks, Dual-mass flywheel, Forecasting, Friction, Friction coefficients, Gear rattle, Human ride comfort modeling, Manual transmissions, Neural networks, NVH, Product development process, Ride comforts, Subjective assessments, Vehicle start-up, Vibrational properties},
	pages = {153--166},
}

