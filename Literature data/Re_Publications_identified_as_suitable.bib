	keywords = {3D modeling, 3D representation, 3d representations, abstract ok, Computer aided design, Computer vision, Conceptual design, Data analysis, Data reduction, Deep learning, Design support, Feature extractor, Generative model, Implicit features, Learning methods, Multiple objects, Point cloud data},
