	keywords = {abstract ok, Aerodynamic designs, Agent based, Classifieds, Constrained optimization, Design spaces, Global optimization, Gradient based, haben wir, Learning algorithms, Machine learning, Metaheuristic optimization, Optimization algorithms, Optimization problems, Search Algorithms, Shape optimization, Topological information, Topology},
}

