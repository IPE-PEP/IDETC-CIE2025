
@article{zhou_machine_2020,
	title = {A {Machine} {Learning} {Approach} to {Customer} {Needs} {Analysis} for {Product} {Ecosystems}},
	volume = {142},
	issn = {1050-0472},
	doi = {10.1115/1.4044435},
	abstract = {Creating product ecosystems has been one of the strategic ways to enhance user experience and business advantages. Among many, customer needs analysis for product ecosystems is one of the most challenging tasks in creating a successful product ecosystem from both the perspectives of marketing research and product development. In this paper, we propose a machine -learning approach to customer needs analysis for product ecosystems by examining a large amount of online user-generated product reviews within a product ecosystem. First, we filtered out uninformative reviews from the informative reviews using a fastText technique. Then, we extract a variety of topics with regard to customer needs using a topic modeling technique named latent Dirichlet allocation. In addition, we applied a rule -based sentiment analysis method to predict not only the sentiment of the reviews but also their sentiment intensity values. Finally, we categorized customer needs related to different topics extracted using an analytic Kano model based on the dissatisfaction -satisfaction pairfrom the sentiment analysis. A case example of the Amazon product ecosystem was used to illustrate the potential and feasibility of the proposed method.},
	number = {1},
	journal = {JOURNAL OF MECHANICAL DESIGN},
	author = {Zhou, F and Ayoub, J and Xu, QL and Yang, XJ},
	month = jan,
	year = {2020},
	keywords = {Design automation, Machine learning, Customer satisfaction, Sentiment analysis, Product design, Statistics, Computer aided design, Kano model, Machine-learning, Sales, Ecosystems, Design theory and methodology, Design for X, Design automations, Machine learning approaches, Customer need, Customer need analyse, Customer needs analysis, Need analysis, Product ecosystems, abstract ok},
}

@article{zhang_concept_2017,
	title = {Concept {Clustering} in {Design} {Teams}: {A} {Comparison} of {Human} and {Machine} {Clustering}},
	volume = {139},
	issn = {1050-0472},
	doi = {10.1115/1.4037478},
	abstract = {Concept clustering is an important element of the product development process. The process of reviewing multiple concepts provides a means of communicating concepts developed by individual team members and by the team as a whole. Clustering, however, can also require arduous iterations and the resulting clusters may not always be useful to the team. In this paper, we present a machine learning approach on natural language descriptions of concepts that enables an automatic means of clustering. Using data from over 1000 concepts generated by student teams in a graduate new product development class, we provide a comparison between the concept clustering performed manually by the student teams and the work automated by a machine learning algorithm. The goal of our machine learning tool is to support design teams in identifying possible areas of "over-clustering" and/or "under-clustering" in order to enhance divergent concept generation processes.},
	number = {11},
	journal = {JOURNAL OF MECHANICAL DESIGN},
	author = {Zhang, CW and Kwon, YP and Kramer, J and Kim, E and Agogino, AM},
	month = nov,
	year = {2017},
	keywords = {abstract ok, Artificial intelligence, Clusterings, Concept clustering, Design process, Design team, Design-process, Embeddings, Learning algorithms, Learning systems, Machine learning, Machine-learning, Product design, Product development, Product development process, Student teams, Students, Team members, Word embedding},
}

@inproceedings{kaliteevskii_concept_2021,
	title = {Concept {Extraction} {Based} on {Semantic} {Models} {Using} {Big} {Amount} of {Patents} and {Scientific} {Publications} {Data}},
	volume = {635},
	isbn = {1868-4238},
	doi = {10.1007/978-3-030-86614-3_11},
	abstract = {Formalisation of heuristic methods for supporting the conceptual design stage of product and technology development has been extensively evolved in industry during the last half of the century and gradually more formally appears in academic context nowadays. Due to the considerable interest from the Industry and the Academia, heuristic approaches such as TRIZ have been strongly developed over the past decades. Thus, TRIZ evolved from a set of empirical inventive principles into a considerably formal approach including techniques for modeling technical problems with the possibility of further overcoming them using formal methods. Moreover, during the last decades, TRIZ has been extensively digitized. Several generations of software have appeared that facilitate the use of inventive methods (Goldfire, Invention Machine). From the trend of digitalisation and the success of machine driven processes, it can be assumed that the further fate of invention methods and formal algorithms for overcoming non-trivial problems lies in the plane of Machine Learning and Artificial Intelligence approaches. The position of the authors is that the idea of automating inventions looks extremely attractive, although in the coming time, digital approaches will rather complement the intelligence of engineers and scientists, rather than replace it. Taking a certain preparatory step towards AI driven inventions, we present a semantic model that can form the basis of future approaches, at the same time, having already sufficient functionality to support the heuristic stage of technology. As part of this work, over 8 millions of patents and scientific publications have been analyzed to extract semantic concepts. Amodelwas built based onMachine Learning methods and Natural Language Processing techniques with the following discussion and application examples.},
	author = {Kaliteevskii, V and Deder, A and Peric, N and Chechurin, L},
	editor = {Borgianni, Y and Brad, S and Cavallucci, D and Livotov, P},
	year = {2021},
	keywords = {abstract ok},
	pages = {141--149},
}

@article{zhang_machine_2021,
